{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Wristband": {
    "AuthConfig": {
      "ClientId": "--some-identifier--",
      "CustomApplicationLoginPageUrl": "",
      "DangerouslyDisableSecureCookies": "false",
      "LoginUrl": "https://{tenant_domain}.mydomain.com/",
      "RedirectUri": "https://{tenant_domain}.mydomain.com/api/auth/callback",
      "RootDomain": "mydomain.com",
      "Scopes": ["openid", "offline_access", "email"],
      "UseCustomDomains": "false",
      "UseTenantSubdomains": "true",
      "WristbandApplicationDomain": "sometest-parent.us.wristband.dev",
      "WristbandTenantDomain": "{tenant_domain}-sometest-parent.us.wristband.dev"
    }
  },
  "AllowedHosts": "*"
}
