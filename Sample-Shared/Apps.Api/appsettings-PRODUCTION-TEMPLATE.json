{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Wristband": {
    "AuthConfig": {
      "ClientId": "--some-identifier--",
      "CustomApplicationLoginPageUrl": "",
      "DangerouslyDisableSecureCookies": "false",
      "LoginUrl": "https://{tenant_domain}.mydomain.com/",
      "RedirectUri": "https://{tenant_domain}.mydomain.com/api/auth/callback",
      "RootDomain": "mydomain.com",
      "Scopes": ["openid", "offline_access", "email"],
      "UseCustomDomains": "false",
      "UseTenantSubdomains": "true",
      "WristbandApplicationDomain": "someprod-parent.us.wristband.dev",
      "WristbandTenantDomain": "{tenant_domain}-someprod-parent.us.wristband.dev"
    },
    "LoginConfig": {
        "DefaultTenantDomain": "someprodone",
        "PostLoginRedirectUrl": "https://{tenant_domain}.mydomain.com/"
    },
    "LogoutConfig": {
      "RedirectUrl": "https://{tenant_domain}.mydomain.com/"
    }
  },
  "AllowedHosts": "*"
}
